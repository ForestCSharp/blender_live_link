
// Flatbuffers IDL file for blender live link
 
namespace Blender.LiveLink;
  
struct Vec3 {
  x:float;
  y:float;
  z:float;
}

struct Vec4 {
  x:float;
  y:float;
  z:float;
  w:float;
}

struct Quat {
  x:float;
  y:float;
  z:float;
  w:float;
}

table Object {
	name:string;
}

table Scene {
	name:string;
	objects:[Object];
}

root_type Scene;
